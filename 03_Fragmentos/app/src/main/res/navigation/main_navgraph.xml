<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navgraph"
    app:startDestination="@id/logeo">

    <fragment
        android:id="@+id/logeo"
        android:name="com.framus.Fragmentos.Logeo"
        android:label="fragment_logeo"
        tools:layout="@layout/fragment_logeo" >
        <action
            android:id="@+id/action_logeo_to_principal"
            app:destination="@id/principal" />
    </fragment>
    <fragment
        android:id="@+id/principal"
        android:name="com.framus.Fragmentos.Principal"
        android:label="fragment_principal"
        tools:layout="@layout/fragment_principal" />
</navigation>